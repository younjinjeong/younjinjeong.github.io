{
  "name": "younjinjeong.github.io",
  "version": "1.0.0",
  "description": "[![Deploy RYONGJIN's Blog to Pages](https://github.com/younjinjeong/younjinjeong.github.io/actions/workflows/hugo.yml/badge.svg)](https://github.com/younjinjeong/younjinjeong.github.io/actions/workflows/hugo.yml)",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:ui": "npx playwright test --ui",
    "test:report": "npx playwright show-report e2e/playwright-report",
    "hugo:build": "hugo --gc --minify",
    "hugo:serve": "hugo server -D",
    "orchestrate": "node orchestrator/index.js",
    "orchestrate:plan": "node orchestrator/index.js --phase plan",
    "orchestrate:build": "node orchestrator/index.js --phase build",
    "orchestrate:test": "node orchestrator/index.js --phase test",
    "orchestrate:review": "node orchestrator/index.js --phase review",
    "cycle": "node orchestrator/index.js",
    "cycle:quick": "node orchestrator/index.js --phase build --phase test",
    "cycle:full": "node orchestrator/index.js --verbose",
    "strapi:fetch": "node scripts/fetch-strapi-content.js",
    "strapi:migrate": "node scripts/migrate-to-strapi.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/younjinjeong/younjinjeong.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/younjinjeong/younjinjeong.github.io/issues"
  },
  "homepage": "https://github.com/younjinjeong/younjinjeong.github.io#readme",
  "devDependencies": {
    "@playwright/test": "^1.57.0"
  }
}
